#include <stdio.h>
#include <conio.h>
struct student
{
    char name[10];
    int age;
};
void main()
{
    struct student s1;
    printf("enter name");
    scanf("%s",s1);
    printf("enter age");
    scanf("%d",&s1);
    printf("/n the information of s1...");
    printf("/n the name is %s",s1);
    printf("/n the age is %d",s1);
}
